1. TLE

class Solution {
    public int[] findBuildings(int[] heights) {
        List<Integer> list = new ArrayList<>();
        
        for (int i = 0; i < heights.length; i++) {
            int j = i + 1;
            boolean flag = true;

            while (j < heights.length) {
                if (heights[j] >= heights[i]) {
                    flag = false;
                    break;
                }
                j += 1;
            }
            
            if (flag) list.add(i);
        }
        
        int[] answer = new int[list.size()];
        for (int i = 0; i < list.size(); i++) {
            answer[i] = list.get(i);
        }
        return answer;
    }
}


